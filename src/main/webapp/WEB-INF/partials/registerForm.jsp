<h2>Create Your Account</h2>
<form action="/register" method="post">
	<%--	username--%>
	<div.form-group>
		<label for="username">Username:</label>
		<input id="username" class="form-input" name="username" type="text">
	</div.form-group>
	<%--	email--%>
	<div.form-group>
		<label for="email">Username:</label>
		<input id="email" class="form-input" name="email" type="email">
	</div.form-group>
	<%--	password--%>
	<div.form-group>
		<label for="password">Username:</label>
		<input id="password" class="form-input" name="password" type="password">
	</div.form-group>
	<%--	confirmPassword--%>
	<div.form-group>
		<label for="confirmPassword">Username:</label>
		<input id="confirmPassword" class="form-input" name="confirmPassword" type="password">
	</div.form-group>
		<div class="form-group">

		</div>
	<button type="submit">Create</button>
</form>

